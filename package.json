{
  "name": "@siamf/teamdesk",
  "version": "1.2.1",
  "description": "A Node.js TeamDesk API client for placing API requests directly to TeamDesk from Node.js. It supports almost all teamdesk api method and query params",
  "homepage": "https://github.com/siamahnaf/teamdesk",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "author": {
    "name": "Siam Ahnaf",
    "email": "mail@siamahnaf.com",
    "url": "https://www.siamahnaf.com"
  },
  "license": "MIT",
  "keywords": [
    "teamdesk",
    "teamdesk client",
    "teamdesk-node-client",
    "teamdesk-js"
  ],
  "repository": {
    "type": "git",
    "directory": "https://github.com/siamahnaf/teamdesk",
    "url": "https://github.com/siamahnaf/teamdesk"
  },
  "devDependencies": {
    "@types/node": "^22.12.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}